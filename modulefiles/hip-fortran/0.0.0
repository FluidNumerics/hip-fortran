#%Module 1.0
#
#  OpenMPI module for use with 'environment-modules' package:
#
conflict                singularity
prepend-path            LD_LIBRARY_PATH             /usr/local/hip-fortran/lib

setenv	HIPFORTRAN_VER "0.0.0"
setenv	HIPFORTRAN_INCLUDE "-I/usr/local/hip-fortran/include"
setenv	HIPFORTRAN_LIB "-L/usr/local/hip-fortran/lib/ -lhipfortran"
